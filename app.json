{
  "expo": {
    "name": "ME-Fisio-Admin",
    "slug": "ME-Fisio-Admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/newLogo.png",
    "splash": {
      "image": "./assets/newLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSRemindersUsageDescription": "To be able to create and update calendars"
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "permissions": ["READ_CALENDAR", "WRITE_CALENDAR"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/newLogo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-noexlogo.png",
          "color": "blue",
          "mode": "production"
        }
      ]
    ],

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
